
url = "https://www.worldometers.info/coronavirus/top10countries"
req = requests.get(url)
bsObj = BeautifulSoup(req.text, "html.parser")
data = bsObj.find_all("div",class_ = "maincounter-number")

print("Total Cases: ", data[0].text.strip())
print("Total Deaths: ", data[1].text.strip())
print("Total Recovered: ", data[2].text.strip())
print("New Cases: ", data[3].text.strip())
print("New Deaths: ", data[4].text.strip())
